{
  "name": "niuma-accelerator",
  "version": "1.0.3",
  "description": "牛马加速器AE插件",
  "scripts": {
    "start": "parcel src/index.html",
    "build:parcel": "parcel build src/index.html --public-url ./ --no-source-maps",
    "build:copy-csxs": "copyfiles -u 1 \"src/CSXS/**/*\" dist",
    "build:copy-jsx": "copyfiles -u 1 \"src/jsx/**/*\" dist",
    "build:copy-assets": "copyfiles -u 1 \"src/assets/**/*\" dist",
    "build:copy-tools": "copyfiles -u 1 \"src/tools/**/*\" dist",
    "build": "npm run build:parcel && npm run build:copy-csxs && npm run build:copy-jsx && npm run build:copy-assets && npm run build:copy-tools"
  },
  "keywords": [
    "CEP",
    "After Effects",
    "Extension"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@parcel/transformer-postcss": "^2.12.0",
    "autoprefixer": "^10.4.19",
    "copyfiles": "^2.4.1",
    "parcel": "^2.12.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3"
  },
  "dependencies": {
    "jquery": "^3.7.1"
  }
}